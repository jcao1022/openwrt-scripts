{
  "local_address": "0.0.0.0",
  "local_port": 1080,
  "protocol": "http",
  "servers": [
    {
      "address": "$SHADOWSOCK_SERVER",
      "fast_open": true,
      "method": "aes-256-gcm",
      "password": "$SHADOWSOCK_PASSWORD",
      "plugin": "obfs-local",
      "plugin-opts": "obfs=http;obfs-host=www.baidu.com",
      "port": $SHADOWSOCK_PORT,
      "timeout": 300
    }
  ]
}

